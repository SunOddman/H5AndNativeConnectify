                              {
                              "type": "QEntryElement",
                              "keyboardType": "Default",
                              "title": "title",
                              "placeholder": "填入分享的标题",
                              "bind": "textValue:binding_title"
                              },
                              {
                              "type": "QEntryElement",
                              "keyboardType": "Default",
