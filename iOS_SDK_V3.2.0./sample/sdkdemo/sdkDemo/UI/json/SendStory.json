                              {
                              "type": "QEntryElement",
                              "keyboardType": "Default",
                              "title": "description",
                              "placeholder": "填入分享的描述",
                              "bind": "textValue:binding_description"
                              },
                              {
                              "type": "QEntryElement",
                              "keyboardType": "URL",
                              "title": "pics",
